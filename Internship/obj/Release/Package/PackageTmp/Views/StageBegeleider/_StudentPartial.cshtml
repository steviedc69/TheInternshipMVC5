@model Internship.Models.Domain.Student

<div class="col-sm-6 col-md-4">
    <div class="thumbnail">
        <img src=@Model.ImageUrl alt="profiel">
        <div class="caption">
            <h3>@Model.Naam @Model.Voornaam</h3>

            <address>
                <strong>Adres :</strong>
                @Model.Adres.StraatNaam @Model.Adres.Nummer<br />
                @Model.Adres.Gemeente.Structuur<br />
                <abbr>Gsm :</abbr> @Model.Gsmnummer<br />
                <a href="mailto:#">@Model.UserName</a>

            </address>

        </div>
    </div>
</div>